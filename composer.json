{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/xcezx/Stream_Filter_Mbstring"
        }
    ],
    "require": {
        "php": "~8.0.0",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "anhskohbo/no-captcha": "^3.0",
        "antlr/antlr4-php-runtime": "^0.7.0",
        "doctrine/dbal": "^2.9",
        "erusev/parsedown": "^1.7",
        "guzzlehttp/guzzle": "^7.4.5",
        "jakeasmith/http_build_url": "^1.0",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.2",
        "laravel/legacy-factories": "^1.3",
        "laravel/passport": "^10.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/csv": "^9.5",
        "misd/linkify": "^1.1",
        "openpear/stream_filter_mbstring": "dev-master",
        "sentry/sentry-laravel": "^3.3.0",
        "staudenmeir/eloquent-eager-limit": "~1.7.1",
        "symfony/css-selector": "^4.3",
        "symfony/dom-crawler": "4.4.37",
        "t1gor/robots-txt-parser": "^0.2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "friendsofphp/php-cs-fixer": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0",
        "spatie/laravel-ignition": "^1.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "fix": [
            "php-cs-fixer fix --config=.php-cs-fixer.dist.php"
        ],
        "test": [
            "phpunit"
        ],
        "annotate": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:model -N",
            "@php artisan ide-helper:meta"
        ],
        "generate-parser": [
            "docker-compose run --rm antlr antlr4 -Dlanguage=PHP -package App\\\\Parser\\\\SearchQuery -o app/Parser/SearchQuery -Xexact-output-dir resources/grammar/SearchQuery.g4"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "symfony/thanks": false
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
