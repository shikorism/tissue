{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/xcezx/Stream_Filter_Mbstring"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "anhskohbo/no-captcha": "^3.0",
        "antlr/antlr4-php-runtime": "^0.5.0",
        "doctrine/dbal": "^2.9",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^7.4",
        "jakeasmith/http_build_url": "^1.0",
        "laravel/framework": "^6.2",
        "laravel/helpers": "^1.2",
        "laravel/passport": "^9.0",
        "laravel/tinker": "^2.0",
        "league/csv": "^9.5",
        "misd/linkify": "^1.1",
        "openpear/stream_filter_mbstring": "dev-master",
        "sentry/sentry-laravel": "^2.4.2",
        "staudenmeir/eloquent-eager-limit": "~1.4.1",
        "symfony/css-selector": "^4.3",
        "symfony/dom-crawler": "^4.3",
        "t1gor/robots-txt-parser": "^0.2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "~2.8.2",
        "facade/ignition": "^1.4",
        "friendsofphp/php-cs-fixer": "2.16.4",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "fix": [
            "php-cs-fixer fix --config=.php_cs.dist"
        ],
        "test": [
            "phpunit"
        ],
        "annotate": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:model -N",
            "@php artisan ide-helper:meta"
        ],
        "generate-parser": [
            "docker-compose run --rm antlr antlr4 -Dlanguage=PHP -package App\\\\Parser\\\\SearchQuery -o app/Parser/SearchQuery -Xexact-output-dir resources/grammar/SearchQuery.g4"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
